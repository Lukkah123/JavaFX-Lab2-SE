<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="394.0" prefWidth="756.0" spacing="20.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.SecondaryController">
    <children>
        <Label text="Balance: " />
      <Label fx:id="balanceId" layoutX="365.0" layoutY="176.0" />
      <TextField fx:id="inputField" prefHeight="25.0" prefWidth="200.0" />
      <Button fx:id="withdrawButton" mnemonicParsing="false" onAction="#withdraw" text="Withdraw" />
        <Button fx:id="secondaryButton" onAction="#switchToPrimary" text="Go Back" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
